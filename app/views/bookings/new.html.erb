<h1>New Booking</h1>

<%= simple_form_for @booking do |f| %>
  <%= f.input :location,
              input_html: { data: { geocoder_target: "location" } },
              wrapper_html: { data: { controller: "geocoder",
geocoder_api_key_value: ENV['MAPBOX_API_KEY'] } } %>

  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
